{
  "name": "angular1-webpack-starter",
  "version": "1.0.0",
  "description": "A starter for angular1 projects",
  "scripts": {
    "start": "npm run serve:dev",
    "build:dev": "rimraf ./dist && webpack --config ./config/webpack.dev.config.js --progress",
    "build:dist": "rimraf ./dist && webpack --config ./config/webpack.dist.config.js --progress",
    "serve:dev": "webpack-dev-server --config ./config/webpack.dev.config.js --hot --inline --progress",
    "serve:dist": "webpack-dev-server --config ./config/webpack.dist.config.js --hot --inline --progress",
    "lint": "tslint './src/**/*.ts' && tslint './src/**/*.js' && tslint './config/**/*.js'",
    "test": "karma start ./config/karma.conf.js",
    "test:watch": "npm test -- --auto-watch --no-single-run",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor ./config/protractor.conf.js"
  },
  "keywords": [],
  "author": "Swen Gorschewski <swen.gorschewski@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/angular": "~1.6.2",
    "@types/angular-mocks": "~1.5.8",
    "@types/jasmine": "~2.5.40",
    "@types/node": "^6.0.57",
    "angular-mocks": "~1.6.1",
    "autoprefixer": "~6.7.0",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "css-loader": "~0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "~0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "~2.26.0",
    "istanbul-instrumenter-loader": "~1.2.0",
    "jasmine-core": "~2.5.2",
    "json-loader": "~0.5.4",
    "karma": "1.4.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.1",
    "less": "~2.7.2",
    "less-loader": "~2.2.3",
    "ng-annotate-loader": "~0.2.0",
    "ng-annotate-webpack-plugin": "~0.1.3",
    "node-sass": "~4.3.0",
    "phantomjs-prebuilt": "~2.1.4",
    "postcss-loader": "~1.2.0",
    "protractor": "~5.0.0",
    "rimraf": "~2.5.4",
    "sass-loader": "~4.1.1",
    "style-loader": "~0.13.1",
    "ts-helpers": "1.1.2",
    "tslint": "~4.3.1",
    "tslint-loader": "^3.3.0",
    "typescript": "~2.1.4",
    "url-loader": "~0.5.7",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0",
    "webpack-merge": "~2.4.0"
  },
  "dependencies": {
    "angular": "~1.6.1",
    "bootstrap-sass": "~3.3.7",
    "core-js": "~2.4.1",
    "ng-metadata": "~4.0.1",
    "rxjs": "~5.0.2"
  }
}
