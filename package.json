{
  "name": "angular-webpack-starter",
  "version": "0.9.1",
  "description": "A starter project for angular and typescript",
  "scripts": {
    "start": "npm run serve:dev",
    "build:dev": "rimraf ./dist && webpack --config ./config/webpack.dev.config.js --progress",
    "build:dist": "rimraf ./dist && webpack --config ./config/webpack.dist.config.js --progress",
    "serve:dev": "webpack-dev-server --config ./config/webpack.dev.config.js --hot --inline --progress",
    "serve:dist": "webpack-dev-server --config ./config/webpack.dist.config.js --hot --inline --progress",
    "test": "karma start ./config/karma.conf.js",
    "test:watch": "npm test -- --auto-watch --no-single-run",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor ./config/protractor.conf.js"
  },
  "keywords": [],
  "author": "Swen Gorschewski <swen.gorschewski@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/angular": "~1.5.20",
    "@types/angular-mocks": "~1.5.8",
    "@types/jasmine": "~2.5.38",
    "@types/node": "^6.0.38",
    "@types/selenium-webdriver": "2.53.31",
    "angular-mocks": "~1.5.9",
    "autoprefixer": "~6.5.3",
    "awesome-typescript-loader": "3.0.0-beta.13",
    "css-loader": "~0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "~0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "~2.24.1",
    "istanbul-instrumenter-loader": "~1.1.0",
    "jasmine-core": "~2.4.1",
    "json-loader": "~0.5.4",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "less": "~2.7.1",
    "less-loader": "~2.2.3",
    "ng-annotate-loader": "~0.2.0",
    "ng-annotate-webpack-plugin": "~0.1.3",
    "node-sass": "~3.13.0",
    "phantomjs-prebuilt": "~2.1.4",
    "postcss-loader": "~1.2.0",
    "protractor": "~4.0.12",
    "rimraf": "~2.5.4",
    "sass-loader": "~4.0.2",
    "style-loader": "~0.13.1",
    "ts-helpers": "1.1.2",
    "ts-node": "~1.7.0",
    "tslint": "~4.0.2",
    "tslint-loader": "^3.3.0",
    "typescript": "~2.1.4",
    "url-loader": "~0.5.7",
    "webpack": "2.2.0-rc.1",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-merge": "~1.0.2"
  },
  "dependencies": {
    "angular": "~1.6.0",
    "bootstrap-sass": "~3.3.7",
    "core-js": "^2.4.1",
    "ng-metadata": "^3.0.3",
    "rxjs": "^5.0.0-rc.1"
  }
}
