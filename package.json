{
  "name": "angular-webpack-starter",
  "version": "0.9.0",
  "description": "A starter project for angular and typescript",
  "scripts": {
    "start": "npm run serve:dev",
    "build:dev": "rimraf ./dist && webpack --config ./config/webpack.config.js",
    "build:dist": "npm run build:dev -- --env.prod",
    "serve:dev": "webpack-dev-server --config ./config/webpack.config.js --hot --inline",
    "serve:dist": "npm run serve:dev -- --env.prod",
    "test": "karma start ./config/karma.conf.js",
    "test:watch": "npm test -- --auto-watch --no-single-run"
  },
  "keywords": [],
  "author": "Swen Gorschewski <swen.gorschewski@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/angular": "^1.5.20",
    "@types/jasmine": "^2.5.38",
    "awesome-typescript-loader": "^3.0.0-beta.9",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.2",
    "karma-jasmine": "^0.3.7",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "ng-annotate-loader": "^0.2.0",
    "node-sass": "^3.13.0",
    "phantomjs-prebuilt": "^2.1.4",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "tslint": "^4.0.2",
    "typescript": "^2.0.10",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.12"
  },
  "dependencies": {
    "angular": "^1.5.9"
  }
}
